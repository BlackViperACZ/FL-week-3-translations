    # mes "[mcname]. This is my family. They're important to me. So, be respectful."
    mes "[mcname]. C'est ma famille. Ils sont importants pour moi. Alors sois respectueux."

# game/code/e10/e10s02.rpy:20
translate french e10s02_d0f44e92:

    # mc "Of course, Min."
    mc "Bien sûr, Min."

