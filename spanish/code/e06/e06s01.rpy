# game/code/e06/e06s01.rpy:95
translate spanish e06s01_0143d3aa:

    # lc "What's wrong?"
    lc "¿Qué ocurre?"

# game/code/e06/e06s01.rpy:98
translate spanish e06s01_6e0cbc41:

    # mc "Nothing?"
    mc "¿Nada?"

# game/code/e06/e06s01.rpy:102
translate spanish e06s01_9afdaa4c:

