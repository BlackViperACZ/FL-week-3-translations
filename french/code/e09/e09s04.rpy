    # mc "See? He's gone. Now, let's grab-"
    mc "Tu vois ? Il est parti. Maintenant, prenons-"

# game/code/e09/e09s04.rpy:271
translate french e09s04_25f2152a:

    # hr "[mcname]-"
    hr "[mcname]-"

