    # sw "*clears throat* And this is my friend. Rose."
    sw "*s'éclaircit la gorge* Et voici mon amie. Rose."

# game/code/e03/e03s02.rpy:174
translate french e03s02_730077fe:

    # sy "Hello Rose."
    sy "Bonjour Rose."

# game/code/e03/e03s02.rpy:176
translate french e03s02_2f48d092:

    # rd "..."
    rd "..."

# game/code/e03/e03s02.rpy:179
