translate italian e08s02_4ae759af:

    # mc "Can you give me a hint about what you want to talk about?"
    mc "Mi dai un indizio su ciò che vuoi dire?"

# game/code/e08/e08s02.rpy:265
translate italian e08s02_ec39b12f:

