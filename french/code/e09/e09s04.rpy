translate french e09s04_69705684:

    # mc "Hana?"
    mc "Hana ?"

# game/code/e09/e09s04.rpy:225
translate french e09s04_d204f323:

    # mc "Hana? What's wrong?"
    mc "Hana ? Qu'est-ce qui ne va pas?"

# game/code/e09/e09s04.rpy:227
translate french e09s04_a6926ac9:

    # hr "He... We... We have to go. We have to leave. Right now."
    hr "Il... nous... nous devons y aller. Nous devons partir.  Maintenant."

# game/code/e09/e09s04.rpy:230
translate french e09s04_87cedd7b:

    # mc "Hana-"
    mc "Hana-"

