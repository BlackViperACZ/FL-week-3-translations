    # hr "Londyn! [mcname]!"
    hr "Londyn !  [mcname] !"

# game/code/e09/e09s04.rpy:342
translate french e09s04_05eaaa65:

    # hr "Come aboard!"
    hr "Bienvenu à bord !"

# game/code/e09/e09s04.rpy:345
translate french e09s04_2bf2731b:

    # lo "Aye, aye, Captain!"
    lo "Ouais ouais, capitaine !"

# game/code/e09/e09s04.rpy:347
