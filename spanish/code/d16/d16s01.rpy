# game/code/d16/d16s01.rpy:226
translate spanish d16s01_25fb8ea8:

    # mc "\"Unknown\"?"
    mc "\"¿Desconocida?\""

# game/code/d16/d16s01.rpy:229
translate spanish d16s01_cf05969f:

    # sy "Bruteforcing these sorts of encryptions take a lot of time. And we can't really predict how long it'll take."
