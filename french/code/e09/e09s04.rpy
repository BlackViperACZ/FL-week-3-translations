    # mc "Say what?"
    mc "Dire quoi ?"

# game/code/e09/e09s04.rpy:189
translate french e09s04_43e224be:

    # hr "Every time-"
    hr "A chaque fois-"

# game/code/e09/e09s04.rpy:193
translate french e09s04_cf3cc3cc:

    # hr "Quick, hide!"
    hr "Merde, vite, cache-toi !"

